# AgriAI Environment Variables
# Copy this file to .env and fill in your values

# Supabase (Get from: https://supabase.com/dashboard/project/_/settings/api)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your-anon-key-here

# WhatsApp Business API (Get from: https://business.facebook.com/wa/manage/home/)
WHATSAPP_ACCESS_TOKEN=your-whatsapp-access-token
WHATSAPP_PHONE_ID=your-phone-number-id
WEBHOOK_VERIFY_TOKEN=your-random-verify-token-123

# Groq AI (Get from: https://console.groq.com/keys)
GROQ_API_KEY=your-groq-api-key

# Optional: Together AI (Alternative free AI)
TOGETHER_API_KEY=your-together-api-key

# Server Configuration
PORT=8000
